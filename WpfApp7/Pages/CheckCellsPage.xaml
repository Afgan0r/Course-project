<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp7.Pages"
      xmlns:WpfApp7="clr-namespace:WpfApp7" x:Class="WpfApp7.Pages.CheckCellsPage"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CheckCellsPage">

    <Page.Resources>
        <WpfApp7:Workshop_EquipmentDataSet1 x:Key="workshop_EquipmentDataSet1"/>
        <CollectionViewSource x:Key="cellViewSource" Source="{Binding Cell, Source={StaticResource workshop_EquipmentDataSet1}}"/>
    </Page.Resources>

    <Grid DataContext="{StaticResource cellViewSource}">
        <DataGrid ItemsSource="{Binding}"/>
    </Grid>
</Page>
